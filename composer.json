{
    "name": "hypersistence/hypersistence-laravel",
    "type": "library",
    "description": "PHP Object Oriented persistence framework to Laravel.",
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/josebatista/hypersistence-laravel"
        }
    ],
    "keywords": ["microframework","framework","persistence","laravel"],
    "license": "MIT",
    "authors": [
        {
            "name": "Mateus Fornari",
            "email": "mateusfornari@gmail.com",
            "homepage": "https://github.com/mateusfornari/hypersistence-alpha"
        }
    ],
    "require": {
        "php": ">=5.3.0"
    },
    "autoload": {
        "psr-4": { "Hypersistence\\": "." }
    },
    "scripts": {
        "post-update-cmd": [
            "php artisan make:command CreateModelsHypersistence"
        ]
    }
}
